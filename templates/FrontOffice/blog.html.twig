    <style>
        /* Likes and Comments Styling */
        .interaction-section {
            border-top: 1px solid #eee;
            border-bottom: 1px solid #eee;
            padding: 20px 0;
            margin: 25px 0;
        }

        .like-btn {
            background: #f8f9fa;
            border: 1px solid #dee2e6;
            border-radius: 20px;
            padding: 8px 20px;
            transition: all 0.3s ease;
            display: inline-flex;
            align-items: center;
        }

        .like-btn:hover {
            background: #e9ecef;
            transform: translateY(-1px);
        }

        .like-btn i {
            margin-right: 8px;
            color: #6c757d;
        }

        .like-count {
            font-weight: 500;
            margin-left: 8px;
            color: #2d2d2d;
        }

        .comments-section {
            margin-top: 30px;
        }

        .comment {
            display: flex;
            align-items: flex-start;
            margin-bottom: 20px;
            padding: 15px;
            background: #f8f9fa;
            border-radius: 8px;
        }

        .comment-avatar {
            width: 40px;
            height: 40px;
            border-radius: 50%;
            margin-right: 15px;
            background: #007bff;
            display: flex;
            align-items: center;
            justify-content: center;
            color: white;
            font-weight: bold;
        }

        .comment-content {
            flex-grow: 1;
        }

        .comment-text {
            margin: 0;
            color: #495057;
            line-height: 1.5;
        }

        .comment-time {
            font-size: 0.85rem;
            color: #6c757d;
            margin-top: 5px;
        }

        .comment-form .form-control {
            border-radius: 8px;
            padding: 12px;
            margin-bottom: 15px;
        }

        .comment-form .btn {
            border-radius: 20px;
            padding: 8px 25px;
            float: right;
        }

        .no-comments {
            text-align: center;
            padding: 20px;
            color: #6c757d;
            font-style: italic;
        }
    </style>
</head>

