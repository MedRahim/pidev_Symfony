<!-- Script AJAX -->
<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
<script>
  $(document).ready(function () {
    $('#searchService').on('input', function () {
      let searchTerm = $(this).val();
      $.ajax({
        url: "{{ path('recherche_service') }}",
        type: "GET",
        data: { nom: searchTerm },
        success: function (data) {
          $('#servicesContainer').html(data);
        },
        error: function () {
          $('#servicesContainer').html('<p class="text-danger">Erreur lors du chargement des services.</p>');
        }
      });
    });
  });
</script>
       
       
       
       
       
       
       
